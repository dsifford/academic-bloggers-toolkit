{
    "borderNone": true,
    "brackets": true,
    "colons": "always",
    "colors": true,
    "commaSpace": true,
    "commentSpace": true,
    "cssLiteral": false,
    "depthLimit": false,
    "duplicates": true,
    "efficient": true,
    "emoji": true,
    "enforceBlockStyle": false,
    "enforceVarStyle": true,
    "extendPref": "extends",
    "globalDupe": true,
    "indentSpaces": 4,
    "leadingZero": true,
    "maxWarnings": false,
    "maxWarningsKill": false,
    "mixed": false,
    "namingConvention": "BEM",
    "namingConventionStrict": true,
    "parenSpace": false,
    "placeholders": false,
    "quotePref": "single",
    "semicolons": true,
    "sortOrder": false,
    "stackedProperties": true,
    "universal": true,
    "valid": true,
    "whitespace": true,
    "zeroUnits": true,
    "zIndexDuplicates": false,
    "zIndexNormalize": false
}
