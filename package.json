{
  "name": "academic-bloggers-toolkit",
  "version": "3.3.0",
  "description": "A plugin extending the functionality of WordPress for Academic Blogging.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd ",
    "clear-cache": "rm -rf node_modules/jest-cli/.haste_cache",
    "chown": "sudo chown -R $USER $(pwd)/dist",
    "update-vendors": "cd vendor && curl -o 'citeproc.js' https://raw.githubusercontent.com/Juris-M/citeproc-js/master/citeproc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsifford/academic-bloggers-toolkit.git"
  },
  "author": "Derek P Sifford",
  "contributors": [
    {
      "name": "Frank Bennett"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dsifford/academic-bloggers-toolkit/issues"
  },
  "homepage": "https://github.com/dsifford/academic-bloggers-toolkit#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.13.0",
    "coveralls": "^2.11.11",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "enzyme": "^2.4.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-file-insert": "^1.0.2",
    "gulp-jade2php": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-stylus": "^2.5.0",
    "gulp-uglify": "^1.5.4",
    "jest-cli": "^14.0.1",
    "nib": "^1.1.0",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.2.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "@types/enzyme": "^1.2.29",
    "@types/jest": "^0.9.27",
    "@types/react": "^0.14.27",
    "@types/react-addons-css-transition-group": "^0.14.13",
    "@types/react-dom": "^0.14.13",
    "@types/sinon": "^1.16.26",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-shallow-compare": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-select": "^1.0.0-beta13",
    "react-virtualized-select": "^1.2.0",
    "typescript": "^2.1.0-dev.20160724"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "compact": true
  },
  "jest": {
    "scriptPreprocessor": "jestPreprocessor.js",
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "sinon",
      "lib/js/utils/Constants"
    ],
    "verbose": true,
    "collectCoverage": true
  }
}
