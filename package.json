{
  "name": "academic-bloggers-toolkit",
  "version": "4.0.1",
  "description": "A plugin extending the functionality of WordPress for Academic Blogging.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd ",
    "lint": "tslint lib/**/*{ts,tsx}",
    "gulp": "./node_modules/.bin/gulp",
    "update-vendors": "cd src/vendor && curl -o 'citeproc.js' https://raw.githubusercontent.com/Juris-M/citeproc-js/master/citeproc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsifford/academic-bloggers-toolkit.git"
  },
  "author": "Derek P Sifford",
  "contributors": [
    {
      "name": "Frank Bennett"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dsifford/academic-bloggers-toolkit/issues"
  },
  "homepage": "https://github.com/dsifford/academic-bloggers-toolkit#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.14.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.12.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-file-insert": "^1.0.2",
    "gulp-jade2php": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sort": "^2.0.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-stylus": "^2.5.0",
    "gulp-uglify": "^2.0.0",
    "gulp-wp-pot": "^1.3.1",
    "jest": "^15.0.1",
    "jest-cli": "^14.2.2-alpha.22bd3c33",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.4",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.14.0-dev.1",
    "tslint-react": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "@types/enzyme": "^2.4.31",
    "@types/jest": "^0.9.29",
    "@types/react": "^0.14.31",
    "@types/react-addons-css-transition-group": "^0.14.15",
    "@types/react-dom": "^0.14.15",
    "@types/sinon": "^1.16.28",
    "mobx": "^2.4.3",
    "mobx-react": "^3.5.5",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-virtualized-select": "^1.3.0",
    "typescript": "^2.1.0-dev.20160724"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "compact": true
  },
  "jest": {
    "scriptPreprocessor": "scripts/jestPreprocessor.js",
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "sinon",
      "mobx",
      "mobx-react",
      "lib/js/utils/Constants",
      "scripts/fixtures"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!**/{vendor,__tests__}/**",
      "!src/lib/js/Frontend.ts",
      "!src/**/index.{ts,tsx}",
      "!src/lib/js/utils/{TinymceFunctions,Externals}.ts"
    ],
    "coverageDirectory": "tmp/coverage",
    "setupFiles": [
      "scripts/beforeTest.js"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "indent": [
        "error",
        4
      ]
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    }
  }
}
